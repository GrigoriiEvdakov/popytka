<% provide :page_title, 'Log In'%>
<%currently_at 'Log In'%>

<%= form_with url: session_path do  |f| %>
<%= f.label :email %>
<%= f.email_field :email, value: params[:email], placeholder:'E-mail'%>
<%= f.label :password%>
<%= f.password_field :password%>
<%= f.check_box :remember_me, value: '1' %>
<%= f.label :remember_me %>
<%= f.submit "Sing In!"%>
<% end %>    
